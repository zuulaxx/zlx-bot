#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/g/Git/zlx-bot/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/g/Git/zlx-bot/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../pngquant-bin@0.1.7/node_modules/pngquant-bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../pngquant-bin@0.1.7/node_modules/pngquant-bin/cli.js" "$@"
fi
