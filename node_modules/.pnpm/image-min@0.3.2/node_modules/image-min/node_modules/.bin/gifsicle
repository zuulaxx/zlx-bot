#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/zuulaxx/Desktop/Git/zlx-bot/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/zuulaxx/Desktop/Git/zlx-bot/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../gifsicle@0.1.7/node_modules/gifsicle/cli.js" "$@"
else
  exec node  "$basedir/../../../../../gifsicle@0.1.7/node_modules/gifsicle/cli.js" "$@"
fi
